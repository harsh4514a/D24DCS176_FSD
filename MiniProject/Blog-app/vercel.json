{
    "version": 2,
    "builds": [
        {
            "src": "client/dist",
            "use": "@vercel/static"
        },
        {
            "src": "server/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/client/dist/index.html"
        }
    ]
}